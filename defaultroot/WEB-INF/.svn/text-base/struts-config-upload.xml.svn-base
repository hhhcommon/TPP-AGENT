<struts-config>
	<form-beans>

		<form-bean name="uploadForm"
			type="com.fordays.fdpay.upload.UploadForm" />
	</form-beans>
	<action-mappings>
		<action name="uploadForm" parameter="thisAction"
			type="com.fordays.fdpay.upload.UploadAction"
			path="/upload/upload" validate="false" scope="request">		
			<forward name="editattachpage" path="/page/editAttach.jsp"></forward>	
		</action>
	</action-mappings>
</struts-config>
