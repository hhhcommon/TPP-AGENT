<?xml version="1.0" encoding="UTF-8"?>
<!-- [syncWithModel=true] -->
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping >

<class 
  name="com.fordays.fdpay.transaction.ExpenseLog"
	table="expense_log"
>

<meta attribute="extends">org.apache.struts.action.ActionForm implements Cloneable</meta>
<meta attribute="class-code"><![CDATA[
  public Object clone()
  {
    Object o = null;
    try
    {
      o = super.clone();
    }
    catch (CloneNotSupportedException e)
    {
      e.printStackTrace();
    }
    return o;
}

private String thisAction="";
 public String getThisAction()
 {
     return thisAction;
 }


public void setThisAction(String thisAction)
 {
     this.thisAction=thisAction;
 }

private int index=0;
 public int getIndex()
 {
     return index;
 }


public void setIndex(int index)
 {
     this.index=index;
 }
 




]]></meta>


    <id
        name="id"
        type="long"
        column="id"
    >
        <generator class="sequence" >
          <param  name="sequence">seq_expenselog</param>
          </generator>
    </id>

    <property
        name="expenseNo"
        column="expense_no"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="amount"
        column="amount"
        type="java.math.BigDecimal"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="content"
        column="content"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="chargeDate"
        column="charge_date"
        type="java.sql.Timestamp"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="status"
        column="status"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="operater"
        column="operater"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>



    <many-to-one
        name="agent"
        class="com.fordays.fdpay.agent.Agent"
        not-null="false"
    >

        <column name="agent_id" />
    </many-to-one>
</class>

</hibernate-mapping>