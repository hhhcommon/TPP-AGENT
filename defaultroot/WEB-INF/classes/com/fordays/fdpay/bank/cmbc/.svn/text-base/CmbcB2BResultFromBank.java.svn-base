package com.fordays.fdpay.bank.cmbc;

import com.fordays.fdpay.bank.ResultFromBank;

/**
 * @民生银行网上银行B2B订单处理结果
 * @version 1.0
 */
public class CmbcB2BResultFromBank extends ResultFromBank {
