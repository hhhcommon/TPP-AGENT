@charset "utf-8";
/* CSS Document */
body { margin:0; padding:0; font-size:12px; font-family:"宋体"; background:#EDF0F4 url(../_img/body_bgx.jpg) repeat-x;}
a img { border:none;}
#warp { width:980px; margin:0 auto; }
#head { padding:2px 15px;}
#head a { border-left:1px solid #999; padding:0 5px 0 10px; color:#005c9c;}
#head .head_a { text-decoration:none; color:#000; border:none;}
.head_img1 { border:1px solid #d6d6d6; padding:1px; margin-right:5px; vertical-align:middle; height:18px; width:18px;}
.head_img2 {vertical-align:middle; margin-left:1px;}
#menu { background:url(../_img/menu_left_bg.jpg) no-repeat left; padding-left:4px;}
#menu_right { background:url(../_img/menu_right_bg.jpg) no-repeat right; padding-right:4px;}
#menu_mid { background:url(../_img/menu_mid_bg.jpg) repeat-x; height:65px;}
.menu_top { padding-top:5px;}
.menu_top ul { margin:0 0 0 25px; padding:0;}
.menu_top ul li { list-style:none; margin:0; padding:0; float:left; width:95px; background:url(../_img/right_a.gif) no-repeat right; text-align:center;}
* html .menu_top ul li { padding-right:8px;}
*+html .menu_top ul li { padding-right:8px;}
.menu_top ul li a { font-weight:bold; font-size:14px; color:#FFF; display:block; text-align:center; text-decoration:none; width:80px; margin-left:6px; height:21px; padding-top:3px;}
.menu_top ul li a:hover { background:url(../_img/menu_top_abg.gif) no-repeat;}
.menu_top .at_menu_top { margin-top:-1px; width:100px;}
.menu_top .at_menu_top a { background:url(../_img/at_menu_a.jpg) no-repeat; width:72px; height:24px;color:#000; padding-top:5px; text-align:left; padding-left:15px;}
.menu_top .at_menu_top a:hover { background:url(../_img/at_menu_a.jpg) no-repeat; width:72px; height:24px;color:#000; padding-top:5px; text-align:left; padding-left:15px;}
.clear { clear:both; margin:0; padding:0; visibility:hidden;}
.menu_bottom { color:#6c6c6c; padding-top:3px;}
.menu_bottom ul { margin:0 0 0 10px; padding:0;}
.menu_bottom ul li { background:url(../_img/right_a1.gif) no-repeat right; list-style:none; float:left; text-align:center; width:97px;}
* html .menu_bottom ul li { padding-right:2px;}
*+html .menu_bottom ul li { padding-right:2px;}
.menu_bottom ul li a { color:#6c6c6c; text-decoration:none; width:90px; height:19px; padding-top:3px; display:block; margin-left:3px;}
.menu_bottom ul li a:hover { background:url(../_img/hover_bg.gif) no-repeat; width:90px;}
.menu_bottom .at_menu_bottoma a { background:url(../_img/at_bottom_abg.gif) no-repeat; color:#000;}
.menu_bottom .at_menu_bottoma a:hover { background:url(../_img/at_bottom_abg.gif) no-repeat; color:#000;}
.main_mid .main_title { font-size:12px;}
.main_mid .main_title p { font-size:12px;}


.main_top { background:#fff url(../_img/main_top_bg.jpg) no-repeat top; padding-top:4px; margin-top:12px;}
.main_bottom { background:url(../_img/main_bottom_bg.jpg) no-repeat bottom; padding-bottom:4px;}
.main_mid { border-left:1px solid #d2d2d2; border-right:1px solid #d2d2d2; padding:4px 8px 4px 8px;}
.main_mid_title { margin:0; padding:8px; background:#e4e4e4;}
.main_mid_title span { float:right;}
.main_mid_title span a { color:#005C9C;}
.main_mid1 p { margin:0; font-size:14px; padding:10px;}
.main_mid1 p a { color:#005C9C; margin-left:20px;}
.font_style1 { font-weight:bold; color:#F00;}
.font_style2 { font-weight:bold; color:#005C9C;}
.btn1 { background:url(../_img/btn1_bg.gif) no-repeat; border:none; font-weight:bold; font-size:12px; width:90px; height:22px;}
#footer { text-align:center; margin-top:30px; color:#666; border-top:2px solid #D3D3D3; padding-top:5px;}
#footer a{ color:#005B9B; }



