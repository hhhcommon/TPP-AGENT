@charset "utf-8";
/* CSS Document */
.activeEMailContainer {
	padding-left: 85px;
	background: url('../images/info_15.gif') no-repeat 15px 0;
}
.activeEMailContainer * {
	font-size: 12px;
}
.activeEMailContainer a {
	color: #005b9b;
	text-decoration: underline;
}
.activeEMailContainer .sucessTips li {
	list-style: disc inside;
	line-height: 30px;
}
.activeEMailContainer h3 {
	font-size: 16px;
	font-weight: bold;
}

.simplyHr {
	margin-bottom: 10px;
}

.restricted td, .restricted th {
	border: 1px solid #ddd;
	height: 30px;
	line-height: 30px;
	width: 125px;
	text-align: left;
	padding-left: 15px;
}
.before {
	background: url('../images/info_16.png') no-repeat 50% -1198px;
	height: 20px;
	width: 20px;
	display: block;
}
.after {
	background: url('../images/info_16.png') no-repeat 50% -898px;
	height: 20px;
	width: 20px;
	display: block;
	border: none;
}
#footer {
	color: #666;
}
.simplyHr {
	margin-top: 20px;
}