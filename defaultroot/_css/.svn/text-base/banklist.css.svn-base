.disappear{ display:none; color:#FFFFFF; } 
.appear{ color:#000000;}
#banking{ margin:0; padding:0; padding-top:5px;}
#banking ul { list-style-type:none; margin:0; padding:0; clear:both;}
#banking li{ float:left; display:block;  width:150px; padding-left:5px; min-height:38px; -height:36px;}
#banking input , .banking{ float:left; margin:0; padding:0; width:23px; height:23px;}
#banking ul li ol{ margin:0; padding:0;}
#banking ul li ol li.bankingS{no-repeat top left; background-position:10px 0px; float:left; margin-left:-1px; width:90%; margin-top:2px; padding-left:20px; font-size:12px;}
.bankingS input{ float:none;}

#banking li img,#banking2 li img,.payWay li img{ border:#CCCCCC 1px solid; }
#banking a:hover {-border:#FF7E00 2px solid;} /*  */
#banking a:active {-border:#FF7E00 2px solid;} /*  */
#banking a:hover img{ -border:0; margin-left:-1px; -margin-top:2px;} /*  */
#banking a:active img{ -border:0; margin-left:-1px; -margin-top:2px;} /*  */
html>body #banking a:hover img{border:#FF9900 2px solid;}
html>body #banking a:active img{border:#FF9900 2px solid;}

.bankingtip{ display:none; background:#FFF69A; font-size:12px;  }
.bankingtip img{ vertical-align:middle;padding:5px 10px 0px 15px;}

#banking a.linkS:hover{border:0px;}
#banking a.linkS:active{border:0px;}
/*Table*/
.tablebanking { border: #B8BAB7 1px solid; border-bottom:  0; border-right: 0; text-align:center;}
.tablebanking td{ border-bottom: #B8BAB7 1px solid; border-right: #B8BAB7 1px solid; padding:0 7px; height:18px; line-height:15px;font-size:12px;}
.tablebanking thead{background:#F5F5F5;}
.tablebanking thead .bankTitle td{ text-align:left;}

