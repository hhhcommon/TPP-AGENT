@charset "utf-8";
/* CSS Document */
body {margin:0;padding:0;font-size:12px;font-family:"宋体", "Arial Narrow";background-color:#fff;}
#container {width:100%;margin:auto;}

/*中间*/
#main {width:506px;height:206px;margin:0 auto;}
#content {width:506px;height:206px;padding:0px;background-color:#FEFFED;border: 1px solid #FFD327}
#content_top{width:506px;height:70px; border-bottom:1px solid #87887A}
#content_t_left{width:50px;height:50px; float:left; padding:10px 0px 0px 40px;}
#content_t_title{width:350px;height:50px; line-height:50px; text-align:center; float:left; padding:10px 0px 0px 20px; font-size:18px; font-weight:bold}
#content_t_right{width:25px;height:20px; float:right; padding:2px 2px 0px 0px;}
#contentMian{width:490px;height:130px; padding:5px 0px 0px 10px}
#contentMian td{ line-height:21px; }

/*字体样式*/
a {color:#0606DA;text-decoration:underline}
a:hover {color:#871378;text-decoration:underline}